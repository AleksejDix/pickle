{
  "name": "usetemporal",
  "version": "2.0.0-alpha.1",
  "description": "Framework-agnostic reactive time library with revolutionary divide() pattern",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./composables": {
      "import": "./dist/composables/index.js",
      "types": "./dist/composables/index.d.ts"
    }
  },
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "LIB_BUILD=true vite build",
    "build:lib": "LIB_BUILD=true vite build",
    "build:app": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint lib src test",
    "lint:fix": "oxlint lib src test --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run format:check && npm run lint"
  },
  "keywords": [
    "time",
    "date",
    "calendar",
    "reactive",
    "temporal",
    "divide",
    "framework-agnostic",
    "typescript",
    "vue",
    "react",
    "vanilla-js"
  ],
  "author": "useTemporal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleksejdix/usetemporal.git"
  },
  "homepage": "https://github.com/aleksejdix/usetemporal#readme",
  "bugs": {
    "url": "https://github.com/aleksejdix/usetemporal/issues"
  },
  "devDependencies": {
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "date-fns": "^4.1.0",
    "jsdom": "^26.1.0",
    "luxon": "^3.7.0",
    "oxlint": "^1.8.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.0.0",
    "vite": "^7.0.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.0",
    "vue-tsc": "^3.0.3"
  },
  "dependencies": {
    "@vue/reactivity": "^3.5.0"
  },
  "peerDependencies": {
    "date-fns": "^4.0.0",
    "luxon": "^3.7.0"
  },
  "peerDependenciesMeta": {
    "date-fns": {
      "optional": true
    },
    "luxon": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
