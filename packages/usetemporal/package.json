{
  "name": "usetemporal",
  "version": "2.0.0-alpha.1",
  "description": "Revolutionary time library with divide() pattern for Vue and beyond",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./composables": {
      "import": "./dist/composables/index.js",
      "types": "./dist/composables/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "time",
    "date",
    "calendar",
    "vue",
    "composables",
    "reactive",
    "temporal",
    "divide",
    "framework-agnostic",
    "typescript"
  ],
  "author": "useTemporal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/usetemporal.git",
    "directory": "packages/usetemporal"
  },
  "homepage": "https://github.com/yourusername/usetemporal#readme",
  "bugs": {
    "url": "https://github.com/yourusername/usetemporal/issues"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "@vue/reactivity": "^3.4.0",
    "date-fns": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 