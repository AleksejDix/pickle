{
  "name": "@usetemporal/adapter-native",
  "version": "2.0.0-alpha.1",
  "type": "module",
  "description": "Native JavaScript Date adapter for useTemporal - zero dependencies",
  "license": "MIT",
  "author": "Aleksej Dix <git@aleksejdix.com>",
  "homepage": "https://github.com/AleksejDix/pickle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksejDix/pickle.git",
    "directory": "packages/adapter-native"
  },
  "bugs": {
    "url": "https://github.com/AleksejDix/pickle/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "dev": "vite build --watch",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "test:coverage": "vitest run --coverage --passWithNoTests"
  },
  "devDependencies": {
    "@types/node": "^22.12.1",
    "@usetemporal/tsconfig": "2.0.0-alpha.1",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@usetemporal/core": "2.0.0-alpha.1"
  },
  "keywords": [
    "usetemporal",
    "adapter",
    "date",
    "native",
    "zero-dependencies"
  ]
}
