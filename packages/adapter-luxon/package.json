{
  "name": "@usetemporal/adapter-luxon",
  "version": "2.0.0-alpha.1",
  "type": "module",
  "description": "Luxon adapter for useTemporal",
  "license": "MIT",
  "author": "useTemporal Contributors",
  "homepage": "https://github.com/usetemporal/usetemporal#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usetemporal/usetemporal.git",
    "directory": "packages/adapter-luxon"
  },
  "bugs": {
    "url": "https://github.com/usetemporal/usetemporal/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
