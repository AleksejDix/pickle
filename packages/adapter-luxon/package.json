  },
  "dependencies": {
    "luxon": "^3.5.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.3",
    "@types/node": "^22.12.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^3.2.4",
    "@usetemporal/tsconfig": "2.0.0-alpha.1"
  },
  "peerDependencies": {
    "@usetemporal/core": "2.0.0-alpha.1"
